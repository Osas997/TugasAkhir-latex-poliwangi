  urldate    = {2025-12-29},
  journal    = {JURNAL SIMBOLIKA: Research and Learning in Communication Study},
  author     = {Arviani, Heidy and Claretta, Dyva and Kusnarto, Kusnarto and Delinda, Nisrina and Izzaanti, Savira},
  month      = apr,
  year       = {2021},
  pages      = {47--56},
