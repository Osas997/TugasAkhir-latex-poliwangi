  urldate  = {2025-12-29},
  journal  = {Jurnal Penelitian Teknologi Informasi dan Sains},
  author   = {{Zaenab Kurnia} and {Amalina Maryam Zakiyyah} and {Nur Qodariyah Fitriyah} and {Agus Milu Susetyo}},
  month    = jun,
  year     = {2024},
  pages    = {115--125}
}

@article{ningrum_pengembangan_2025,
  title     = {{PENGEMBANGAN} {BOT} {KOMENTAR} {OTOMATIS} {DENGAN} {ANALISIS} {SENTIMEN} {BERBASIS} {BERT} {PADA} {TIKTOK} {UNTUK} {UMKM} {DI} {LHOKSEUMAWE}},
  volume    = {10},
  copyright = {http://creativecommons.org/licenses/by-nc-sa/4.0},
  issn      = {2502-891X, 2477-2062},
  url       = {http://jurnal.univrab.ac.id/index.php/rabit/article/view/6152},
  doi       = {10.36341/rabit.v10i2.6152},
  abstract  = {UMKM di Kota Lhokseumawe menghadapi tantangan dalam merespons komentar pelanggan secara cepat dan konsisten di TikTok akibat keterbatasan sumber daya. Penelitian ini bertujuan untuk membangun sistem bot komentar otomatis berbasis Natural Language Processing (NLP) dengan algoritma BERT (Bidirectional Encoder Representations from Transformers). Sistem dirancang untuk mengklasifikasikan komentar berdasarkan sentimen (positif, negatif, netral) dan pertanyaan umum, lalu membalas secara otomatis. Proses melibatkan scraping komentar TikTok, preprocessing data, pelatihan model IndoBERT, evaluasi performa menggunakan metrik F1-score, precision, recall, dan akurasi. Hasil evaluasi menunjukkan akurasi mencapai 91,94\%, membuktikan efektivitas model dalam memahami konteks dan memberikan respons relevan. Sistem ini diimplementasikan menggunakan Python dan Selenium dan diharapkan dapat membantu meningkatkan efisiensi interaksi pelanggan dan memperkuat strategi pemasaran digital.},
  number    = {2},
  urldate   = {2025-12-29},
  journal   = {Rabit : Jurnal Teknologi dan Sistem Informasi Univrab},
  author    = {Ningrum, Aqiilah Cahya and {Rizal Tjut Adek} and {Nunsina}},
  month     = jul,
  year      = {2025},
  pages     = {431--444}
}

@article{murtiyoso_systematic_2025,
  title     = {A {Systematic} {Review} of {Retrieval}-{Augmented} {Generation} for {Enhancing} {Domain}-{Specific} {Knowledge} in {Large} {Language} {Models}},
  volume    = {9},
  copyright = {http://creativecommons.org/licenses/by-nc/4.0},
  issn      = {2541-2019, 2541-044X},
  url       = {https://jurnal.polgan.ac.id/index.php/sinkron/article/view/14824},
  doi       = {10.33395/sinkron.v9i2.14824},
  abstract  = {This literature review examines the use of Retrieval-Augmented Generation (RAG) in enhancing Large Language Models (LLM) for domain-specific knowledge. RAG integrates retrieval techniques with generative models to access external knowledge sources, addressing the limitations of LLMs in handling specialized information. By leveraging external data, RAG improves the accuracy and relevance of generated content, making it particularly useful in fields that require detailed and up-to-date knowledge. This review highlights the effectiveness of RAG in overcoming challenges such as data sparsity and the dynamic nature of specialized knowledge. Furthermore, it discusses the potential of RAG to enhance LLM performance, scalability, and the ability to generate contextually accurate responses in knowledge-intensive applications. Key challenges and future research directions in the implementation of RAG for domain-specific knowledge are also identified.},
  number    = {2},
  urldate   = {2025-12-29},
  journal   = {Sinkron},
  author    = {Murtiyoso, Murtiyoso and Tahyudin, Imam and Berlilana, Berlilana},
  month     = jun,
  year      = {2025},
  pages     = {969--977}
}

