@article{pokhrel_practical_2025,
  title      = {A {Practical} {Application} of {Retrieval}-{Augmented} {Generation} for {Website}-{Based} {Chatbots}: {Combining} {Web} {Scraping}, {Vectorization}, and {Semantic} {Search}},
  volume     = {6},
  issn       = {2582-4104},
  shorttitle = {A {Practical} {Application} of {Retrieval}-{Augmented} {Generation} for {Website}-{Based} {Chatbots}},
  url        = {https://irojournals.com/tcsst/article/view/6/4/7},
  doi        = {10.36548/jtcsst.2024.4.007},
  abstract   = {The Retrieval-Augmented Generation (RAG) model significantly enhances the capabilities of large language models (LLMs) by integrating information retrieval with text generation, which is particularly relevant for applications requiring context-aware responses based on dynamic data sources. This research study presents a practical implementation of a RAG model personalized for a Chabot that answers user inquiries from various specific websites. The methodology encompasses several key steps: web scraping using BeautifulSoup to extract relevant content, text processing to segment this content into manageable chunks, and vectorization to create embeddings for efficient semantic search. By employing a semantic search approach, the system retrieves the most relevant document segments based on user queries. The OpenAI API is then utilized to generate contextually appropriate responses from the retrieved information. Key results highlight the system's effectiveness in providing accurate and relevant answers, with evaluation metrics centered on response quality, retrieval efficiency, and user satisfaction. This research contributes a comprehensive integration of scraping, vectorization, and semantic search technologies into a cohesive chatbot application, offering valuable insights into the practical implementation of RAG models.},
  language   = {en},
  number     = {4},
  urldate    = {2025-12-29},
  journal    = {Journal of Trends in Computer Science and Smart Technology},
  author     = {Pokhrel, Sangita and K C, Bina and Shah, Prashant Bikram},
  month      = jan,
  year       = {2025},
  pages      = {424--442},
  file       = {Full Text PDF:C\:\\Users\\NITRO\\Zotero\\storage\\7Z3RHSJ2\\Pokhrel et al. - 2025 - A Practical Application of Retrieval-Augmented Generation for Website-Based Chatbots Combining Web.pdf:application/pdf}
}

